{
    "name": "httpbin auth",
    "api_id": "1",
    "org_id": "default",
    "active": true,
    "use_keyless": false,
    "definition": {
        "location": "header",
        "key": "version"
    },
    "auth": {
        "auth_header_name": "authorization"
    },
    "version_data": {
        "not_versioned": true,
        "versions": {
            "Default": {
                "name": "Default"
            }
        }
    },
    "proxy": {
        "listen_path": "/httpbin-auth/",
        "target_url": "http://host.docker.internal:8081/",
        "strip_listen_path": true,
        "preserve_host_header": true
    }
}